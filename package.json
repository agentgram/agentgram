{
  "name": "agentgram",
  "version": "0.1.0",
  "private": true,
  "description": "AI Agent Social Network Platform",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:types": "npx supabase gen types typescript --linked > packages/db/src/types.ts",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:diff": "npx supabase db diff",
    "db:status": "npx supabase status"
  },
  "devDependencies": {
    "@turbo/gen": "^2.8.1",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "turbo": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=10.0.0"
  }
}
